{
  "name": "radh-ui",
  "version": "2.1.0",
  "description": "RADH UI",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/radh-ui/radh-ui.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "genversion -e -s src/components/radh-version/version.js && npm-run-all clean build:stencil build:storybook post:build",
    "start": "genversion -e -s src/components/radh-version/version.js && npm-run-all --parallel stencil storybook",
    "test": "cross-env STENCIL_ENV=dev stencil test --spec",
    "test.watch": "cross-env STENCIL_ENV=dev stencil test --spec --watchAll",
    "generate": "plop",
    "storybook": "start-storybook -p 6007 -s ./stencil-utilities/public-assets",
    "build-storybook": "build-storybook",
    "clean": "del-cli dist storybook-static",
    "build:stencil": "cross-env STENCIL_ENV=prod stencil build --docs",
    "build:storybook": "build-storybook",
    "stencil": "cross-env STENCIL_ENV=dev stencil build --dev --watch --serve --docs",
    "prestart": "npm run build:stencil",
    "post:build": "node ./stencil-utilities/post-build.js",
    "storybook:publish": "build-storybook -c .storybook -s www -o sbwww",
    "deploy": "npm run storybook:publish && firebase deploy",
    "publish": "npm run build && npm publish"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@blaze/atoms": "^9.2.0",
    "@ionic/core": "4.11.7",
    "@stencil/core": "^1.8.2",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-notes": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/html": "^5.2.8",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/d3-selection": "^1.4.1",
    "@types/jest": "^24.0.25",
    "@types/node": "12.12.22",
    "babel-loader": "^8.0.6",
    "chalk": "^3.0.0",
    "chart.js": "^2.9.3",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "d3": "^5.14.2",
    "del-cli": "^3.0.0",
    "firebase": "^7.6.1",
    "fs-extra": "^8.1.0",
    "genversion": "^2.2.0",
    "lodash.kebabcase": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "plop": "^2.5.3",
    "rxfire": "^3.9.6",
    "rxjs": "^6.5.3",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "license": "MIT",
  "dependencies": {},
  "stencil": {
    "protocol": "http",
    "host": "localhost",
    "port": 3333,
    "buildDir": "build"
  }
}
