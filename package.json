{
  "name": "radh-ui",
  "version": "2.3.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/radh-ui/radh-ui.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "genversion -e -s src/components/radh-version/version.js && stencil build --docs",
    "start": "genversion -e -s src/components/radh-version/version.js && stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 -s ./www",
    "build-storybook": "npm run build && build-storybook -o sbwww && cp -r ./www/assets ./sbwww/assets",
    "deploy": "npm run build-storybook && firebase deploy"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@stencil/core": "^1.12.2",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/html": "^5.3.19",
    "@types/chart.js": "^2.9.21",
    "@types/jest": "24.9.1",
    "@types/puppeteer": "2.0.1",
    "babel-loader": "^8.1.0",
    "chart.js": "^2.9.3",
    "d3": "^5.16.0",
    "firebase": "^7.15.1",
    "firebase-tools": "^8.4.2",
    "genversion": "^2.2.1",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "puppeteer": "2.1.1",
    "rxfire": "^3.13.1"
  },
  "license": "MIT",
  "dependencies": {}
}
